{
  "name": "next-app-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:dev": "powershell -ExecutionPolicy Bypass -File ./scripts/dev.ps1",
    "deploy": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy.ps1",
    "deploy:fast": "cross-env DOCKER_BUILDKIT=1 COMPOSE_DOCKER_CLI_BUILD=1 powershell -ExecutionPolicy Bypass -File ./scripts/deploy.ps1",
    "deploy:clean": "powershell -ExecutionPolicy Bypass -File ./scripts/clean.ps1 && npm run deploy:fast",
    "prune": "docker system prune -af --volumes"
  },
